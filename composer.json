{
    "name": "nativecli/nativecli",
    "description": "CLI helper for NativePHP Projects",
    "require": {
        "guzzlehttp/guzzle": "^7.9",
        "symfony/console": "^7.2",
        "symfony/process": "^7.2",
        "laravel/installer": "^5.11",
        "illuminate/support": "^11.41",
        "illuminate/filesystem": "^11.41",
        "z4kn4fein/php-semver": "^3.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "larastan/larastan": "^3.0"
    },
    "bin": [
        "bin/nativecli"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NativeCLI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NativeCLI\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Pete Bishop",
            "email": "peter@soniaandpete.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable"
}
